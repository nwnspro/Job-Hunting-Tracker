# 数据库配置
DATABASE_URL="postgresql://username:password@localhost:5432/landed_db"

# JWT配置
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRES_IN="7d"

# 服务器配置
PORT=3001
NODE_ENV="development"

# CORS配置
CORS_ORIGIN="http://localhost:5173"

# 速率限制配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Google Sheets API Configuration
# You need to create a Google Cloud project and enable Google Sheets API
# Then create a service account and download JSON key file
GOOGLE_SERVICE_ACCOUNT_KEY_FILE="./google-service-account-key.json"

# Or use environment variables for Google API authentication
# GOOGLE_CLIENT_EMAIL="your-service-account@project.iam.gserviceaccount.com"
# GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
